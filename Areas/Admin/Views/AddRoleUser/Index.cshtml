@model List<ProjectShopIdentity.Data.AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Danh sách người dùng dang ky saler</h2>

@if (Model.Count == 0)
{
    <p>Không có người dùng saler.</p>
}
else
{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Tên người dùng</th>
                <th>Email</th>
                <th>Thao tac</th>
                <!--Thêm các cột khác tùy ý-->
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td>

                    </td>
                </tr>
            }
        </tbody>
    </table>
}

